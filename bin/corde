#!/usr/bin/env node

'use strict';

const MINIMAL_NODE_VERSION = 12;

var ver = process.versions.node;
var majorVer = parseInt(ver.split('.')[0], MINIMAL_NODE_VERSION);

// Version required for Discord v12
if (majorVer < MINIMAL_NODE_VERSION) {
  console.error(`Node version '${ver}' is not supported. Please use Node.js ${MINIMAL_NODE_VERSION} or higher.`);
  process.exit(1);
} else {
  require = require('esm')(module /*, options*/);
  const { main } = require('../lib/src/cli');
  main(process.argv);
}
