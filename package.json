{
  "name": "corde",
  "version": "0.1.0",
  "description": "A simple library for Discord bot tests",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "directories": {
    "lib": "bin"
  },
  "bin": "bin/corde",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "keywords": [
    "cli",
    "typescript",
    "nodejs",
    "discord-bot",
    "end-to-end",
    "test"
  ],
  "scripts": {
    "start": "node bin/corde",
    "create": "build && yarn test",
    "build": "tsc -p .",
    "watch": "tsc -w",
    "test": "jest",
    "test-with-coverage": "jest --collect-coverage --collectCoverageFrom=src/**/*.ts --collectCoverageFrom='!src/errors/*.ts'",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "tslint": "tslint --fix 'src/**/*.ts'",
    "np": "np --no-cleanup --no-yarn",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint --project tsconfig.json -t stylish",
    "kill": "./bin/kill.bat"
  },
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^14.0.5",
    "@types/rewire": "^2.5.28",
    "@types/ws": "^7.2.4",
    "babel-jest": "^26.0.1",
    "cli-spinners": "^2.3.0",
    "codecov": "^3.6.5",
    "husky": "^4.2.3",
    "jest": "^25.3.0",
    "nodemon": "^2.0.0",
    "np": "^6.2.0",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "rewire": "^5.0.0",
    "source-map-support": "^0.5.16",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "arg": "^4.1.3",
    "chalk": "^4.0.0",
    "discord.js": "^12.2.0",
    "esm": "^3.2.25",
    "figlet": "^1.2.4",
    "ora": "^4.0.4",
    "path": "^0.12.7",
    "rxjs": "^6.5.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasgmagalhaes/corde.git"
  },
  "author": {
    "name": "lucasgmagalhaes",
    "email": "lucasgsm88@gmail.com",
    "url": "https://github.com/lucasgmagalhaes"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lucasgmagalhaes/corde/issues"
  },
  "homepage": "https://github.com/lucasgmagalhaes/corde#readme"
}
