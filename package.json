{
  "name": "corde",
  "version": "1.0.2",
  "description": "A simple library for Discord bot tests",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "directories": {
    "lib": "bin"
  },
  "bin": "bin/corde",
  "keywords": [
    "discord-bot",
    "discord-js",
    "nodejs",
    "testing",
    "discord",
    "corde",
    "test",
    "cli",
    "typescript-library",
    "typescript",
    "automated-testing",
    "bot"
  ],
  "scripts": {
    "start": "node bin/corde",
    "prepublishOnly": "./scripts/publish.sh",
    "build": "bash ./scripts/build.sh",
    "watch": "bash ./scripts/watch.sh",
    "test": "jest",
    "test-with-coverage": "jest --collect-coverage --collectCoverageFrom=src/**/*.ts --collectCoverageFrom='!src/errors/*.ts'",
    "refresh": "rm -rf ./node_modules ./package-lock.json && yarn install",
    "tslint": "tslint --fix 'src/**/*.ts' 'tests/**/*.ts",
    "np": "np --no-cleanup --no-yarn",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint --project tsconfig.json -t stylish"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@types/esm": "^3.2.0",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.22",
    "@types/rewire": "^2.5.28",
    "@types/ws": "^7.2.4",
    "babel-jest": "^26.0.1",
    "cli-spinners": "^2.3.0",
    "codecov": "^3.6.5",
    "dotenv": "^8.2.0",
    "husky": "^4.2.3",
    "jest": "^25.3.0",
    "jest-mock-process": "^1.4.0",
    "nodemon": "^2.0.0",
    "np": "^6.2.0",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "rewire": "^5.0.0",
    "source-map-support": "^0.5.16",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "arg": "^4.1.3",
    "chalk": "^4.0.0",
    "commander": "^5.1.0",
    "discord.js": "^12.2.0",
    "esm": "^3.2.25",
    "figlet": "^1.2.4",
    "ora": "^4.0.4",
    "path": "^0.12.7",
    "rxjs": "^6.5.5"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasgmagalhaes/corde.git"
  },
  "author": {
    "name": "Lucas Gomes",
    "email": "lucasgsm88@gmail.com",
    "url": "https://github.com/lucasgmagalhaes"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lucasgmagalhaes/corde/issues"
  },
  "homepage": "https://github.com/lucasgmagalhaes/corde#readme",
  "files": [
    "lib/",
    "bin/"
  ]
}
