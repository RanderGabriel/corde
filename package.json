{
  "name": "conncord",
  "version": "0.1.0-alpha.1",
  "description": "conncord is a typescript library to create automated tests for discord bots",
  "main": "./lib/src/index.js",
  "directories": {
    "lib": "bin"
  },
  "bin": {
    "conncord": "./lib/src/cli.js"
  },
  "engines": {
    "node": ">=0.10"
  },
  "keywords": [
    "cli",
    "typescript",
    "nodejs",
    "discord-bot",
    "end-to-end",
    "test"
  ],
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts'",
    "cli": "node lib/src/cli.js --run",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "watch": " tsc -w",
    "test": "npm i -g && conncord",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "barrels": "barrelsby -c barrelsby.json",
    "tslint": "tslint --fix 'src/**/*{.ts,.tsx}'",
    "np": "np --no-cleanup --no-yarn",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint --project tsconfig.json -t stylish"
  },
  "devDependencies": {
    "@types/clear": "^0.1.0",
    "@types/figlet": "^1.2.0",
    "@types/node": "^12.7.3",
    "barrelsby": "^2.1.1",
    "nodemon": "^1.18.7",
    "np": "^5.0.3",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "typescript": "^3.6.2",
    "tslint-config-prettier": "^1.18.0",
    "prettier": "^1.16.4"
  },
  "dependencies": {
    "@types/shelljs": "^0.8.5",
    "chalk": "^2.4.1",
    "clear": "^0.1.0",
    "commander": "^3.0.1",
    "discord.js": "^11.4.2",
    "figlet": "^1.2.4",
    "path": "^0.12.7",
    "shelljs": "^0.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasgmagalhaes/conncord.git"
  },
  "author": {
    "name": "lucasgmagalhaes",
    "email": "lucasgsm88@gmail.com",
    "url": "https://github.com/lucasgmagalhaes"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lucasgmagalhaes/conncord/issues"
  },
  "homepage": "https://github.com/lucasgmagalhaes/conncord#readme"
}
