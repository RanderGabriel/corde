{
  "name": "concord",
  "version": "0.0.1",
  "description": "concord is a typescript library to create automated tests for discord bots",
  "main": "./lib/src/index.js",
  "directories": {
    "lib": "bin"
  },
  "bin": {
    "concord": "./lib/src/cli.js"
  },
  "keywords": [
    "cli",
    "typescript",
    "nodejs",
    "discord-bot",
    "end-to-end",
    "test"
  ],
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec ts-node src/cli.ts",
    "cli": "node lib/cli.js --run",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "build-watch": " tsc -w",
    "test": "npm i -g && concord",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "generate-barrels": "barrelsby -c barrelsby.json"
  },
  "devDependencies": {
    "@types/clear": "^0.1.0",
    "@types/figlet": "^1.2.0",
    "@types/node": "^12.7.1",
    "barrelsby": "^2.1.1",
    "nodemon": "^1.18.7",
    "ts-node": "^8.3.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "@types/shelljs": "^0.8.5",
    "chalk": "^2.4.1",
    "clear": "^0.1.0",
    "commander": "^3.0.0",
    "discord.js": "^11.4.2",
    "figlet": "^1.2.1",
    "path": "^0.12.7",
    "shelljs": "^0.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasgmagalhaes/engage.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lucasgmagalhaes/engage/issues"
  },
  "homepage": "https://github.com/lucasgmagalhaes/engage#readme"
}
