{
  "name": "corde",
  "version": "0.1.0-beta.0",
  "description": "corde is a typescript library to create automated tests for discord bots",
  "main": "./lib/src/index.js",
  "directories": {
    "lib": "bin"
  },
  "bin": {
    "corde": "bin/corde"
  },
  "publishConfig": {
    "acess": "public"
  },
  "engines": {
    "node": ">=0.10"
  },
  "keywords": [
    "cli",
    "typescript",
    "nodejs",
    "discord-bot",
    "end-to-end",
    "test"
  ],
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts'",
    "run-cli": "node lib/src/cli.js --run",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "watch": " tsc -w",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "tslint": "tslint --fix 'src/**/*{.ts,.tsx}'",
    "np": "np --no-cleanup --no-yarn",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint --project tsconfig.json -t stylish"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/node": "^12.7.4",
    "husky": "^4.2.3",
    "nodemon": "^2.0.0",
    "np": "^5.0.3",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.8.1",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "discord.js": "^11.4.2",
    "figlet": "^1.2.4",
    "path": "^0.12.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasgmagalhaes/corde.git"
  },
  "author": {
    "name": "lucasgmagalhaes",
    "email": "lucasgsm88@gmail.com",
    "url": "https://github.com/lucasgmagalhaes"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lucasgmagalhaes/corde/issues"
  },
  "homepage": "https://github.com/lucasgmagalhaes/corde#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
