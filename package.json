{
  "name": "trybot",
  "version": "0.1.0-alpha.0",
  "description": "trybot is a typescript library to create automated tests for discord bots",
  "main": "./lib/src/index.js",
  "directories": {
    "lib": "bin"
  },
  "bin": {
    "trybot": "./lib/src/cli.js"
  },
  "keywords": [
    "cli",
    "typescript",
    "nodejs",
    "discord-bot",
    "end-to-end",
    "test"
  ],
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts'",
    "cli": "node lib/src/cli.js --run",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "watch": " tsc -w",
    "test": "npm i -g && trybot",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "barrels": "barrelsby -c barrelsby.json",
    "tslint": "tslint --fix 'src/**/*{.ts,.tsx}'",
    "np": "np"
  },
  "devDependencies": {
    "@types/clear": "^0.1.0",
    "@types/figlet": "^1.2.0",
    "@types/node": "^12.7.2",
    "barrelsby": "^2.1.1",
    "nodemon": "^1.18.7",
    "np": "^5.0.3",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@types/shelljs": "^0.8.5",
    "chalk": "^2.4.1",
    "clear": "^0.1.0",
    "commander": "^3.0.0",
    "discord.js": "^11.4.2",
    "figlet": "^1.2.1",
    "path": "^0.12.7",
    "shelljs": "^0.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasgmagalhaes/trybot.git"
  },
  "author": "lucasgmagalhaes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lucasgmagalhaes/trybot/issues"
  },
  "homepage": "https://github.com/lucasgmagalhaes/trybot#readme"
}
