{
  "name": "corde",
  "version": "2.0.4",
  "description": "A simple library for Discord bot tests",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "directories": {
    "lib": "bin"
  },
  "bin": "bin/corde",
  "keywords": [
    "discord-bot",
    "discord-js",
    "nodejs",
    "testing",
    "discord",
    "corde",
    "test",
    "cli",
    "typescript-library",
    "typescript",
    "automated-testing",
    "bot"
  ],
  "scripts": {
    "clearbuild": "gulp clear",
    "build": "bash tasks.sh build",
    "build:prod": "gulp",
    "build:publish": "bash ./scripts/publish.sh",
    "prettier-lib": "prettier --write ./lib",
    "watch": "bash tasks.sh watch",
    "release": "release-it",
    "corde": "node ./bin/corde",
    "killProcess": "cd ./etc && bash ./build_and_kill.sh",
    "e2e": "jest --config ./e2e/jest.config.ts",
    "test": "jest --silent --detectOpenHandles",
    "test-with-coverage": "jest --silent --ci --detectOpenHandles --collect-coverage --collectCoverageFrom=src/**/*.ts",
    "refresh": "rm -rf ./node_modules ./package-lock.json && yarn install",
    "fix-lint": "tslint --fix 'src/**/*.ts' 'tests/**/*.ts",
    "np": "np --no-cleanup --no-yarn",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint --project tsconfig.json -t stylish",
    "validate-ci": "circleci config validate",
    "jestClear": "jest --clearCache",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-syntax-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-syntax-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@microsoft/tsdoc": "^0.13.0",
    "@types/esm": "^3.2.0",
    "@types/glob": "^7.1.3",
    "@types/gulp": "^4.0.8",
    "@types/gulp-babel": "^6.1.29",
    "@types/gulp-strip-comments": "^2.5.0",
    "@types/jest": "^26.0.0",
    "@types/merge-stream": "^1.1.2",
    "@types/node": "^14.0.22",
    "@types/require-from-string": "^1.2.0",
    "@types/rewire": "^2.5.28",
    "@types/rimraf": "^3.0.0",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.2.4",
    "babel-jest": "^26.0.1",
    "cli-spinners": "^2.4.0",
    "codecov": "^3.6.5",
    "dotenv": "^8.2.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-strip-comments": "^2.5.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "pinst": "^2.1.6",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.0.0",
    "release-it": "^14.4.1",
    "require-from-string": "^2.0.2",
    "rewire": "^5.0.0",
    "semver": "^7.3.2",
    "source-map-support": "^0.5.16",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.3",
    "yarn": "^1.22.10"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "commander": "^7.0.0",
    "discord.js": "^12.2.0",
    "esm": "^3.2.25",
    "glob": "^7.1.6",
    "jest-diff": "^26.6.2",
    "log-update": "^4.0.0",
    "ora": "^5.0.0",
    "pretty-format": "^26.6.2",
    "tslib": "^2.1.0",
    "uuid": "^8.3.1"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasgmagalhaes/corde.git"
  },
  "jest": {
    "projects": [
      "./jest.config.js",
      "./e2e/jest.config.js"
    ]
  },
  "author": {
    "name": "Lucas Gomes",
    "email": "lucasgsm88@gmail.com",
    "url": "https://github.com/lucasgmagalhaes"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lucasgmagalhaes/corde/issues"
  },
  "homepage": "https://corde.netlify.app",
  "files": [
    "lib/**",
    "bin/",
    "index.js",
    "index.d.ts"
  ]
}
